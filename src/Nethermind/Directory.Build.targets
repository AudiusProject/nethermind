  <ItemGroup Label="MonitorPackagePart2" Condition="($(ProjectName.EndsWith('Test'))==false) AND '$(TargetFramework)'!='netstandard2.1' AND '$(ProjectName)'!='Nethermind.Monitoring.Generator' AND '$(ProjectName)'!='Nethermind.Monitoring.Interceptor'">
